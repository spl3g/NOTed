{
	"name": "noted-api",
	"version": "1.0.50",
	"description": "A simple note-taking API with sharing capabilities",
	"scripts": {
		"dev": "bun run --watch src/index.ts",
		"build": "bun build src/index.ts --outdir dist",
		"start": "bun run dist/index.js",
		"test": "bun test",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@bogeychan/elysia-logger": "^0.1.10",
		"@elysiajs/cors": "latest",
		"@elysiajs/jwt": "latest",
		"@elysiajs/openapi": "latest",
		"elysia": "latest",
		"postgres": "^3.4.3"
	},
	"devDependencies": {
		"bun-types": "latest",
		"db-migrate": "^0.11.14",
		"db-migrate-pg": "^1.5.2"
	},
	"module": "src/index.js"
}
